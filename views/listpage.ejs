<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr>
            <th>game</th><th>creator</th><th>Added By</th><th>Likes</th><th>Actions</th>
        </tr>

        <% items.forEach(function(item){  %>

<tr>
        <td><%= item.game%></td>
     <td><%= item.creator%></td>
     <td><%=item.addedby%></td>
     <td><%=item.likes%></td>
     <td><%=item.actions%></td>
</tr>



        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
